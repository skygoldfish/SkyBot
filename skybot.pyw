def calc_pivot(jl, jh, jc, do):
    if jl > 0 and jh > 0 and jc > 0 and do > 0:
        tmp = (jl + jh + jc)/3 + (do - jc)
        pivot = round(tmp, 2)
        return pivot
    else:
        return 0

