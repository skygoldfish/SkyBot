                        del 근월물_선물_현재가_버퍼[:]
                    else:
                        pass

                    근월물_선물_현재가_버퍼.append(근월물_선물_현재가)

                    if max(근월물_선물_현재가_버퍼) > 0:
                        df_futures_cm_graph.at[cme_time_index, 'high'] = max(근월물_선물_현재가_버퍼)
                    else:
